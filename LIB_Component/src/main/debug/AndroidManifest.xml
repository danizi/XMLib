<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xm.lib.component">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".test.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".test.DialogActivity" />
        <activity android:name=".test.PopActivity" />
        <activity android:name=".test.TreeViewActivity" />
        <activity android:name=".test.SignViewActivity" />
        <activity android:name=".test.CharActivity" />
        <activity android:name=".test.TipActivity" />
    </application>

</manifest>