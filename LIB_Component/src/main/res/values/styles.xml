<resources xmlns:ns1="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="suibowen" parent="android:Widget">
        <item name="android:stateListAnimator" ns1:ignore="NewApi,PrivateResource">@animator/design_appbar_state_list_animator</item>
        <item name="android:keyboardNavigationCluster" ns1:ignore="NewApi">true</item>
        <item name="android:touchscreenBlocksFocus" ns1:ignore="NewApi">true</item>
        <item name="colorControlHighlight">@color/textPrimaryRedBk</item>
    </style>
</resources>
